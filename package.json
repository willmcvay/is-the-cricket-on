    "isomorphic-unfetch": "^2.0.0",
    "next": "^6.1.2",
    "next-with-apollo": "^3.1.3",
    "react": "^16.5.2",
    "react-apollo": "^2.1.4",
    "react-dom": "^16.5.2",
    "redis": "^2.8.0",
