{
  "name": "is-the-cricket-on",
  "version": "1.0.0",
  "description": "An app for cricket fans",
  "main": "./server/index.ts",
  "repository": "https://github.com/willmcvay/is-the-cricket-on",
  "author": "Will McVay <willmcvay@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "yarn clean-cache && nodemon server/index.ts --watch server --exec ts-node --typeCheck --compilerOptions '{\"module\":\"commonjs\"}'",
    "build": "NODE_ENV=production next build && tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production node .next/production-server/index.js",
    "clean-cache": "rm -rf ./node_modules/.cache && rm -rf ./cache-loader",
    "lint": "tslint --fix && ./node_modules/.bin/prettier --write"
  },
  "private": false,
  "dependencies": {
    "apollo-boost": "0.1.16",
    "apollo-server-express": "2.1.0",
    "dotenv": "6.0.0",
    "express": "4.16.3",
    "graphql": "14.0.2",
    "graphql-tools": "4.0.0",
    "isomorphic-unfetch": "3.0.0",
    "next": "7.0.0",
    "next-with-apollo": "3.1.3",
    "react": "16.5.2",
    "react-apollo": "2.1.11",
    "react-dom": "16.5.2",
    "redis": "2.8.0"
  },
  "devDependencies": {
    "@babel/core": "7.1.0",
    "@babel/preset-env": "7.1.0",
    "@babel/types": "7.0.0",
    "@types/body-parser": "1.17.0",
    "@types/express": "4.16.0",
    "@types/next": "6.1.6",
    "@types/node": "10.10.1",
    "@types/react": "16.4.14",
    "@types/react-dom": "16.0.7",
    "@types/redis": "2.8.6",
    "@zeit/next-typescript": "1.1.1",
    "babel-loader": "8.0.2",
    "babel-plugin-import-graphql": "2.6.2",
    "graphql-tag": "2.9.2",
    "husky": "0.14.3",
    "lint-staged": "7.3.0",
    "nodemon": "1.18.4",
    "prettier": "1.14.3",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-config-standard": "8.0.1",
    "typescript": "3.0.3",
    "typescript-babel-jest": "1.0.5"
  },
  "lint-staged": {
    "linters": {
      "*.{ts,tsx}": ["yarn lint", "git add"]
    }
  }
}
